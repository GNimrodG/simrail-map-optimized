{
  "name": "smo-backend",
  "version": "1.0.0",
  "author": "GNimrodG <g.nimrod.g@data-unknown.com>",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --watch 'src' --cls --cache-directory tmp --respawn --transpile-only src/index.ts",
    "build:no-sentry": "tsc",
    "build": "tsc && yarn sentry:sourcemaps",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org data-unknown --project smo-backend ./dist && sentry-cli sourcemaps upload --org data-unknown --project smo-backend ./dist",
    "prod": "node dist/index.js",
    "prod:migrate": "prisma migrate deploy && yarn prod"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.13.0",
    "dotenv": "^16.4.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@opentelemetry/instrumentation-socket.io": "^0.40.0",
    "@prisma/client": "^5.14.0",
    "@sentry/cli": "^2.32.1",
    "@sentry/node": "^8.7.0",
    "@sentry/profiling-node": "^8.7.0",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "logform": "^2.6.0",
    "lru-cache": "^10.2.2",
    "notepack.io": "^3.0.1",
    "prisma": "^5.14.0",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.5",
    "socket.io-msgpack-parser": "^3.0.2",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.8",
    "utf-8-validate": "^6.0.4"
  }
}
